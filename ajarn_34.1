#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() 
{
    char a[50];
    char trimmed_a[50];
    char rev_a[50];
    int i;
    int j=0;

    printf("Enter your string : ");
	gets(a);
    
	for (i = 0; i < strlen(a); i++)
    {
        if (isalnum(a[i]))
        {
            trimmed_a[j] = a[i];
            j++;
        }
    }
    trimmed_a[j] = '\0';
    
    j = 0;
    do{
        trimmed_a[j] = tolower(trimmed_a[j]);
        j++;
    }while(trimmed_a[j]);
    
   strcpy(rev_a,trimmed_a);
   strrev(rev_a);

    if( strcmp(trimmed_a,rev_a) == 0)
	{
		printf("Palindrome");
	}
	else
	{
		printf("Not Palindrome");
	}
	return 0;
}
